	@Override
	protected void onClose()
	{
		super.onClose();

		this.startColor.disable();
		this.endColor.disable();
	}

	@Override
	protected boolean subContains(float mouseX, float mouseY)
	{
		return super.subContains(mouseX, mouseY) || this.startColor.contains(mouseX, mouseY) || this.endColor.contains(mouseX, mouseY);
	}

	public boolean shouldLockInput()
	{
		return this.startColor.shouldLockInput() || this.endColor.shouldLockInput();
	}
	
	public UserInputLine getRotation()
	{
		return rotation;
	}
	
	public WrapperTexture getWrapper()
	{
		return wrapper;
	}
