	@Override
	protected Vector2f getHandOffset(boolean focused, EntityAction currentAction, int currentImage)
	{
		if (focused)
		{
			handOffset.x = handOffsets[2];
		}
		else
		{
			handOffset.x = handOffsets[0];
		}

		handOffset.y = handOffsets[1];

		return handOffset;
	}

