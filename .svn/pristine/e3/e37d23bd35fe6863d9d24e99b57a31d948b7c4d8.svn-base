		endColorButton.render(container, 1.0f, g);

		g.setColor(Color.black);
		g.draw(endColorButton.getButtonBox());

		addColor.render(container, 1.0f, g);
		addColorButton.render(container, 1.0f, g);

		g.setColor(Color.black);
		g.draw(addColorButton.getButtonBox());

		colorDistrModeButton.render(container, alphaFactor, g);

		Renderer.clearBind();
	}

